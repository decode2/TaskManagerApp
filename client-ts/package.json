{
  "name": "client-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "2.2.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/dom": "10.4.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.126",
    "@types/react": "19.0.12",
    "@types/react-calendar": "3.9.0",
    "@types/react-dom": "19.0.4",
    "axios": "1.12.2",
    "date-fns": "^4.1.0",
    "framer-motion": "12.6.3",
    "jwt-decode": "4.0.0",
    "lucide-react": "0.487.0",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-big-calendar": "^1.19.4",
    "react-calendar": "5.1.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^7.4.0",
    "react-scripts": "5.0.1",
    "react-toastify": "11.0.5",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 react-scripts start",
    "start:local": "react-scripts start",
    "start:network": "cross-env REACT_APP_NETWORK_MODE=local HOST=0.0.0.0 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:watch": "react-scripts test --watchAll",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "version:check": "node ../scripts/check-version.js check",
    "version:info": "node ../scripts/check-version.js info",
    "version:fix": "node ../scripts/check-version.js fix",
    "version:bump": "node ../scripts/version.js",
    "release": "node ../scripts/release.js",
    "pre-commit": "node ../scripts/pre-commit.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "10.4.21",
    "cross-env": "10.0.0",
    "cypress": "^15.1.0",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.3"
  }
}
